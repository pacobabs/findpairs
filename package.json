{
  "name": "findpair",
  "version": "1.0.0",
  "description": "NextJS React Typescript Redux application with Ant Design and Styled components",
  "main": "index.js",
  "keywords": [
    "NextJS",
    "React",
    "Typescript",
    "Redux"
  ],
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "serve": "npx serve out",
    "export": "next build && next export",
    "test": "jest --coverage --verbose --colors",
    "lint": "eslint *.{js,ts,tsx} --ignore-path .gitignore .",
    "lint:fix": "eslint *.{js,ts,tsx} --quiet --fix --ignore-path .gitignore ."
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix --ignore-path .gitignore .",
      "cross-env NODE_ENV=test jest --bail --findRelatedTests",
      "git add"
    ]
  },
  "author": "pacobabs",
  "license": "ISC",
  "dependencies": {
    "next": "^11.1.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@types/node": "^14.0.23",
    "@types/react": "^16.9.43",
    "@types/react-redux": "^7.1.9",
    "@types/styled-components": "^5.1.1",
    "@types/webpack-env": "^1.15.2",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "antd": "^4.4.2",
    "babel-eslint": "^10.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "cross-env": "^7.0.2",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "styled-components": "^5.1.1",
    "typescript": "^3.9.7"
  }
}
